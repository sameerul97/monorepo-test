    "@storybook/react": "^6.4.19",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "config": "*",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@storybook/addon-docs": "^6.4.19",
    "tsup": "^5.11.13"
  },
  "scripts": {
    "build": "tsup src/index.tsx --legacy-output --minify --format esm,cjs --dts --external react",
